{
  "compilerOptions": {
    "allowJs": false,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "target": "es2016",
    "module": "commonjs",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ]
}