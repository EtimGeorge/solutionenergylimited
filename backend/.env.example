
# PostgreSQL Database Configuration
# Replace with your actual database credentials from cPanel
DB_USER="yourcpaneluser_dbuser"
DB_HOST="localhost"
DB_NAME="yourcpaneluser_submissions"
DB_PASSWORD="your_db_password"
DB_PORT=5432

# Email (Nodemailer) Configuration
# Replace with your SMTP provider's details (e.g., from Namecheap)
SMTP_HOST="smtp.yourprovider.com"
SMTP_PORT=465
SMTP_SECURE=true
SMTP_USER="your_email@yourdomain.com"
SMTP_PASS="your_email_password"

# Email Content
EMAIL_FROM_NAME="SEESL Website"
EMAIL_FROM_ADDRESS="noreply@yourdomain.com"
EMAIL_TO="recipient@yourdomain.com"

# Google reCAPTCHA v3 Configuration
# Get these keys from the Google reCAPTCHA Admin Console
RECAPTCHA_SITE_KEY="your_recaptcha_site_key_for_frontend"
RECAPTCHA_SECRET_KEY="your_recaptcha_secret_key_for_backend"

# Server Configuration
PORT=3000
